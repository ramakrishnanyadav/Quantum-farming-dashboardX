# Quantum Smart Farming Dashboard - Environment Variables Template
# Copy this file to .env and add your actual API keys

# =============================================================================
# WEATHER DATA API
# =============================================================================
# Get your free API key from: https://openweathermap.org/api
OPENWEATHER_API_KEY=your_openweather_api_key_here


# =============================================================================
# MARKET DATA API
# =============================================================================
# Get free API key from: https://www.alphavantage.co/
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================
# Default farm location (lat, lon)
DEFAULT_FARM_LAT=19.0760
DEFAULT_FARM_LON=72.8777

# Quantum simulation settings
DEFAULT_QUBITS=4
DEFAULT_CIRCUIT_DEPTH=3
DEFAULT_QUANTUM_SHOTS=1024

# Cache settings (hours)
WEATHER_CACHE_DURATION=1
SOIL_CACHE_DURATION=24
QUANTUM_CACHE_DURATION=6

# Debug mode
DEBUG=False
LOG_LEVEL=INFO
